{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ApiKeyAuthentication": {
    "Enabled": false,
    "ValidApiKeys": ["your-secret-api-key-here"],
    "HeaderName": "X-API-Key"
  },
  "Cors": {
    "AllowedOrigins": [],
    "AllowedMethods": [
      "GET",
      "POST",
      "PUT",
      "PATCH",
      "DELETE",
      "OPTIONS"
    ],
    "AllowedHeaders": [
      "Content-Type",
      "X-API-Key",
      "Authorization"
    ],
    "AllowCredentials": false,
    "PreflightMaxAge": 86400
  }
}
